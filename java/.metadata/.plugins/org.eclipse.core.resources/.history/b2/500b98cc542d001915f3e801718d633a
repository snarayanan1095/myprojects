package algo_hw4;

public class Hashcode32Bit {
	

	long hashCode=0;
	
	
	public long hashCode(String str) {

		  int n=str.length();
		 
		 // hashCode = str.hashCode();
		        for(int i=0;i<n;i++) {
		        	//hashCode= (hashCode) +((long)str.charAt(i)*(Math.pow(31, (n-1-i)));
		        	hashCode+=str.charAt(i)*Math.pow(31, n-1-i);
		        }
		    
		    return hashCode;
	   }
	
	public static void main(String args[]) {
		Hashcode32Bit bit=new Hashcode32Bit();
		System.out.println(bit.hashCode("He"));
		
	}

}
